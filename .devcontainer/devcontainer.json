{
  "name": "Webots Ready Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [6080],
  "remoteUser": "vscode",
  "postCreateCommand": "mkdir -p ~/.vnc && x11vnc -storepasswd webots ~/.vnc/passwd",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python"]
    }
  }
}
